{
  "name": "my-imook-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "initsqldev": "cross-env STAGE_ENV=dev node src/database/mysql/initsql",
    "initsqltest": "cross-env STAGE_ENV=test node src/database/mysql/initsql",
    "initsqlsim": "cross-env STAGE_ENV=sim node src/database/mysql/initsql",
    "initsqlprod": "cross-env STAGE_ENV=prod node src/database/mysql/initsql",
    "startdev": "cross-env STAGE_ENV=dev PORT=9001 node bin/www",
    "starttest": "cross-env STAGE_ENV=test PORT=9002 node bin/www",
    "startsim": "cross-env STAGE_ENV=sim PORT=9003 node bin/www",
    "startprod": "cross-env STAGE_ENV=prod PORT=9004 node bin/www",
    "dev": "pm2 restart ecosystem.config.js --only my-imook-server_dev",
    "test": "pm2 restart ecosystem.config.js --only my-imook-server_test",
    "sim": "pm2 restart ecosystem.config.js --only my-imook-server_sim",
    "prod": "pm2 restart ecosystem.config.js --only my-imook-server_prod",
    "stopdev": "pm2 stop my-imook-server_dev",
    "stoptest": "pm2 stop my-imook-server_test",
    "stopsim": "pm2 stop my-imook-server_sim",
    "stopprod": "pm2 stop my-imook-server_prod"
  },
  "dependencies": {
    "busboy": "0.2.14",
    "captchapng": "0.0.1",
    "cross-env": "5.0.5",
    "debug": "2.6.3",
    "ejs": "2.3.3",
    "formstream": "1.1.0",
    "koa": "2.3.0",
    "koa-bodyparser": "3.2.0",
    "koa-convert": "1.2.0",
    "koa-json": "2.0.2",
    "koa-jsonp": "2.0.2",
    "koa-logger": "2.0.1",
    "koa-onerror": "3.1.0",
    "koa-router": "7.1.1",
    "koa-session": "5.5.0",
    "koa-static": "3.0.0",
    "koa-views": "5.2.1",
    "log4js": "1.1.1",
    "multiparty": "4.1.3",
    "mysql": "2.15.0",
    "request": "2.83.0",
    "request-promise": "4.2.2",
    "require-directory": "2.1.1",
    "xss": "0.3.4"
  },
  "devDependencies": {
    "nodemon": "1.8.1"
  }
}
